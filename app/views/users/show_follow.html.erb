<div class="main top-main">
	<div class="top-message">
		<div class="row">
			<div class="col-md-4">
				<section class="user_info">
					<%= render 'shared/user_info' %>
				</section>
				<section class="stats">
					<%= render 'shared/stats' %>
				</section>
				
				
				
			</div>
			<div class="col-md-8">
				<h3><%= @title %> (<%= @users.count %>)</h3>
				
				
			
				<ul class="users">
					<%= render @users %>
				</ul>
				
			</div>
		</div>
	</div>
</div>