<div class="main users-new">
  <div class="container">
    <div class="form-heading">Edit your category</div>
    <div class="form users-form">
      

      <div class="form-body">
        <%= form_tag("/admin/categories/#{@category.id}", {multipart: true, method: :patch}) do %>
          <p>Category</p>
          <input name="title" value="<%= @category.title %>">
          <input type="submit" value="Edit">
        <% end %>
      </div>
    <div>
  </div>
</div>