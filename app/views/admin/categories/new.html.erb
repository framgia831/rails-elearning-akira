<div class="main users-new">
  <div class="container">
    <div class="form-heading">Create new categories</div>
    <div class="form users-form">
      

      <div class="form-body">
        <%= form_tag("/admin/categories") do %>
            <div class="form-group">
            <%= label_tag :title %>
            <%= text_field_tag :title%>
            </div>
            <div class="form-group">
            <%= label_tag :description %>
            <%= text_field_tag :description %>
            </div>
                <%= submit_tag "Create"%>
        <% end %>

        
        
      </div>
    <div>
  </div>
</div>